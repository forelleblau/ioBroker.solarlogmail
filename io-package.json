    "authors": [
      "forelleblau <marceladam@gmx.ch>"
    ],
    "keywords": [
      "energy",
      "solar"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "main": "main.js",
    "icon": "solarlogmail.png",
    "enabled": true,
    "extIcon": "https://raw.githubusercontent.com/forelleblau/ioBroker.solarlogmail/master/admin/solarlogmail.png",
    "readme": "https://github.com/forelleblau/ioBroker.solarlogmail/blob/master/README.md",
    "loglevel": "info",
    "mode": "daemon",
    "type": "energy",
    "compact": true,
    "connectionType": "cloud",
    "dataSource": "poll",
    "materialize": true,
    "supportCustoms": true,
    "dependencies": [{
      "js-controller": ">=1.4.2"
    }]
  },
  "encryptedNative": [
    "password"
  ],

  "native": {
    "username": "username",
    "password": "password",
    "host": "0.0.0.0",
    "port": 993,
    "tls": false,
    "mailbox": "INBOX",
    "searchFilter": "Solar-Log",
    "markSeen": true,
    "fetchUnreadOnStart": false
  },


  "objects": [],
  "instanceObjects": [{
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.SN",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Serial Number",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.lastmaildate",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Datum Mail Ertragsübersicht",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.lastmailtime",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Zeit Mail Ertragsübersicht",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    }
  ]
}
